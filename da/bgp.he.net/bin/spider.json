{
    "task_name": "bgp.he.net",
    "rules": [
        {
            "url_match": "http://bgp.he.net",
            "item_rule": {
                "scope_rule": "(?s).*",
                "kv_rule": {
                    "html": "(?s)(.*)"
                }
            }
        }
    ],
    "output_file": "bgp.he.net.txt",
    "config": {
        "polling_time": "100ms",
        "wait_time": "100ms",
        "download_timeout": "5s",
        "connection_timeout": "2s",
        "concurrency": 500,
        "max_retry_times": 20,
        "succ": "BGP",
        "headers": {
            "User-Agent": "Mozilla/5.0 (X11; Linux i686) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/41.0.2272.35 Safari/537.36"
        }
    }
}
