{
    "task_name": "bgp.he.net",
    "rules": [
        {
            "url_match": "http://bgp.he.net",
            "item_rule": {
                "scope_rule": "(?s).*",
                "kv_rule": {
                    "html": "(?s)(.*)"
                }
            }
        }
    ],
    "config": {
        "polling_time": "100ms",
        "wait_time": "100ms",
        "download_timeout": "20s",
        "connection_timeout": "20s",
        "concurrency": 1000,
        "max_retry_times": 20,
        "succ": "Domain Name:",
        "headers": {
            "User-Agent": "Mozilla/5.0 (X11; Linux i686) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/41.0.2272.35 Safari/537.36"
        }
    }
}
